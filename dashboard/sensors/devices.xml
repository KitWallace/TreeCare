<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <fields>
        <field>
            <id>moisture</id>
            <title>Raw moisture</title>
            <min>0</min>
            <max>3000</max>
            <interval>500</interval>
            <colour>red</colour>
        </field>
        <field>
            <id>moisture-pc</id>
            <title>Moisture</title>
            <unit>%</unit>
            <min>0</min>
            <max>100</max>
            <interval>10</interval>
            <colour>lightblue</colour>
            <bands>
                <band threshold="90">
                    <colour>darkgreen</colour>
                    <text>wet</text>
                </band>
                <band threshold="60">
                    <colour>green</colour>
                    <text>very moist</text>
                </band>
                <band threshold="40">
                    <colour>palegreen</colour>
                    <text>moist</text>
                </band>
                <band threshold="2">
                    <colour>orange</colour>
                    <text>dry</text>
                </band>
                <band threshold="0">
                    <colour>grey</colour>
                    <text>out</text>
                </band>
            </bands>
        </field>
        <field>
            <id>moisture-pc2</id>
            <title>Moisture</title>
            <unit>%</unit>
            <min>0</min>
            <max>100</max>
            <interval>10</interval>
            <colour>lightblue</colour>
            <bands>
                <band threshold="90">
                    <colour>darkgreen</colour>
                    <text>wet</text>
                </band>
                <band threshold="60">
                    <colour>green</colour>
                    <text>very moist</text>
                </band>
                <band threshold="40">
                    <colour>palegreen</colour>
                    <text>moist</text>
                </band>
                <band threshold="2">
                    <colour>orange</colour>
                    <text>dry</text>
                </band>
                <band threshold="0">
                    <colour>grey</colour>
                    <text>out</text>
                </band>
            </bands>
        </field>
        <field>
            <id>battery-voltage</id>
            <title>Battery Level</title>
            <unit>volt</unit>
            <min>3</min>
            <max>4.4</max>
            <interval>0.2</interval>
            <decimals>1</decimals>
            <colour>black</colour>
            <bands>
                <band threshold="3.7">
                    <colour>green</colour>
                    <text>good</text>
                </band>
                <band threshold="3.5">
                    <colour>orange</colour>
                    <text>low</text>
                </band>
                <band threshold="0">
                    <colour>red</colour>
                    <text>dead</text>
                </band>
            </bands>
        </field>
        <field>
            <id>temp-C</id>
            <title>Soil Temperature</title>
            <unit>째C</unit>
            <min>-5</min>
            <max>45</max>
            <interval>5</interval>
            <colour>orange</colour>
        </field>
        <field>
            <id>soil-temp-C</id>
            <title>Soil Temp</title>
            <unit>째C</unit>
            <min>-5</min>
            <max>45</max>
            <interval>5</interval>
            <colour>orange</colour>
        </field>
        <field>
            <id>air-temp-C</id>
            <title>Air Temp</title>
            <unit>째C</unit>
            <min>-5</min>
            <max>45</max>
            <interval>5</interval>
            <colour>red</colour>
        </field>
        <field>
            <id>humidity-temp</id>
            <title>Humidity sensor temp</title>
            <unit>째C</unit>
            <min>-5</min>
            <max>45</max>
            <interval>5</interval>
            <colour>red</colour>
        </field>
        <field>
            <id>humidity</id>
            <title>Humidity</title>
            <unit>%</unit>
            <min>0</min>
            <max>100</max>
            <interval>10</interval>
            <colour>purple</colour>
        </field>
        <field>
            <id>rainfall</id>
            <title>Hourly Rainfall</title>
            <unit>mm</unit>
            <min>0</min>
            <max>10</max>
            <interval>1</interval>
            <colour>blue</colour>
        </field>
        <field>
            <id>UV</id>
            <title>light level</title>
            <unit>W/m<sup>2</sup>
            </unit>
            <min>0</min>
            <max>10</max>
            <interval>1</interval>
            <colour>red</colour>
        </field>
        <field>
            <id>baro</id>
            <title>Barometric Pressure</title>
            <unit>mb</unit>
            <min>980</min>
            <max>1040</max>
            <interval>10</interval>
            <colour>pink</colour>
        </field>
        <field>
            <id>rssi</id>
            <title>WiFi RSSI</title>
            <unit>db</unit>
            <min>-100</min>
            <max>0</max>
            <interval>10</interval>
            <colour>brown</colour>
            <link>https://www.arduino.cc/en/Reference/WiFiRSSI</link>
        </field>
        <field>
            <id>signal-quality</id>
            <title>Signal Quality</title>
            <min>0</min>
            <max>25</max>
            <interval>5</interval>
            <colour>brown</colour>
        </field>
        <field>
            <id>run_ms</id>
            <title>Run time</title>
            <unit>ms</unit>
            <min>15000</min>
            <max>40000</max>
            <interval>5000</interval>
            <colour>purple</colour>
        </field>
        <field>
            <id>ms</id>
            <title>Sample time</title>
            <unit>ms</unit>
            <min>0</min>
            <max>200000</max>
            <interval>20000</interval>
            <colour>black</colour>
        </field>
        <field>
            <id>seconds</id>
            <title>Sample time</title>
            <unit>sec</unit>
            <min>0</min>
            <max>250</max>
            <interval>50</interval>
            <colour>black</colour>
        </field>
        <field>
            <id>Grid</id>
            <title>Grid current</title>
            <unit>amp</unit>
            <min>0</min>
            <max>2</max>
            <interval>0.5</interval>
            <colour>red</colour>
            <decimals>1</decimals>
        </field>
        <field>
            <id>Corrected_Grid</id>
            <title>Corrected Grid current</title>
            <unit>amp</unit>
            <min>-2</min>
            <max>2</max>
            <interval>0.5</interval>
            <decimals>1</decimals>
            <colour>purple</colour>
        </field>
        <field>
            <id>Power_Used</id>
            <title>Power used</title>
            <unit>watt</unit>
            <min>0</min>
            <max>3</max>
            <interval>0.5</interval>
            <colour>blue</colour>
            <decimals>1</decimals>
        </field>
        <field>
            <id>Solar</id>
            <title>Solar current</title>
            <unit>amp</unit>
            <min>0</min>
            <max>3</max>
            <interval>0.5</interval>
            <colour>green</colour>
            <decimals>1</decimals>
        </field>
        <field>
            <id>current</id>
            <title>Current</title>
            <unit>amp</unit>
            <min>0</min>
            <max>20</max>
            <interval>2</interval>
            <colour>red</colour>
        </field>
        <field>
            <id>watts</id>
            <title>Power</title>
            <unit>milliwatt</unit>
            <min>0</min>
            <max>6000</max>
            <interval>1000</interval>
            <colour>blue</colour>
        </field>
        <field>
            <id>boot-no</id>
            <title>Boot No</title>
            <min>0</min>
            <max>3000</max>
            <interval>500</interval>
            <colour>blue</colour>
        </field>
    </fields>
